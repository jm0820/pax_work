<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

<!--    设备信息-->
    <TextView
        android:id="@+id/deviceMessage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="deviceMessage"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

<!--   执行按钮-->

    <Button
        android:id="@+id/button1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="168dp"
        android:layout_marginBottom="176dp"
        android:text="excute"
        android:singleLine="true"
        tools:ignore="MissingConstraints" />
    <!--    cmd命令输入框-->
    <EditText
        android:id="@+id/cmdInput"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:contentDescription="输入命令行框"
        android:singleLine="true"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="please input cmd"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="164dp"
        tools:layout_editor_absoluteY="455dp" />

    <!--    cmd结果返回框-->
    <TextView
        android:id="@+id/cmdresult"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:contentDescription="命令行返回结果"
        android:singleLine="true"
        android:text="result init"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="174dp"
        tools:layout_editor_absoluteY="602dp" />

</androidx.constraintlayout.widget.ConstraintLayout>